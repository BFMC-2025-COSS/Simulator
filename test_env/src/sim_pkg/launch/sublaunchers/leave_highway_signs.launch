<?xml version="1.0" encoding="UTF-8"?>

<launch>

  <!-- LEAVE HIGHWAY SIGNS -->
  <node
      name  = "LHIGH_U"
      pkg   = "gazebo_ros"
      type  = "spawn_model"
      args  = "-file $(find models_pkg)/leave_highway_sign/model.sdf -sdf -model LHIGH_U -x 13.558288 -y 13.558288 -z 0 -R 0  -P 0  -Y 3.13"/>
  
  <node
      name  = "LHIGH_Z"
      pkg   = "gazebo_ros"
      type  = "spawn_model"
      args  = "-file $(find models_pkg)/leave_highway_sign/model.sdf -sdf -model LHIGH_Z -x 7.069463 -y 13.141230 -z 0 -R 0  -P 0  -Y 0"/>

